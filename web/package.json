{
  "name": "web",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "version": "2.1.0-beta",
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:css": "npm run generate:css",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "npm-run-all -p dev:*",
    "dev:css": "npm run generate:css",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css",
    "serve": "remix-serve build",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch"
  },
  "dependencies": {
    "@portabletext/react": "^1.0.6",
    "@remix-run/node": "1.5.1",
    "@remix-run/react": "1.5.1",
    "@remix-run/serve": "1.5.1",
    "@remix-run/vercel": "^1.5.1",
    "@sanity/image-url": "^1.0.1",
    "@types/react-syntax-highlighter": "^15.5.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.3.3",
    "picosanity": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gist": "^1.2.4",
    "react-syntax-highlighter": "^15.5.0",
    "node-persist": "^3.1.0",
    "web-push": "^3.4.5",
    "npm-run-all": "^4.1.5",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.5",
    "@types/node-persist": "^3.1.2"
  },
  "engines": {
    "node": ">=16"
  }
}